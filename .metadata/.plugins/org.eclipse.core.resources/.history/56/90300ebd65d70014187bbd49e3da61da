<%@page import="servlet.vo.Member"%>
<%@page import="servlet.vo.Address"%>
<%@page import="java.util.HashMap"%>
<%@page import="java.util.ArrayList"%>
<%@ page contentType="text/html;charset=UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>ForEach</title>
</head>
<body>
<%
int[] nums = {10,20,30,40,50,60,70,80,90};
request.setAttribute("nums", nums);

ArrayList<Member> strList = new ArrayList<Member>();
strList.add(new Member("id-1","1111","홍길동",22,false,new Address("ad-1","123-123","부산")));
request.setAttribute("list", strList);

HashMap<String,String> strMap = new HashMap<String,String>();
strMap.put("1","a");
strMap.put("2","b");
strMap.put("3","c");
strMap.put("4","d");
strMap.put("5","e");
strMap.put("6","f");
request.setAttribute("map",strMap);
%>
<%int i=0; %>
<c:forEach items="${requestScope.nums }" var="num">
	nums<%=++i %> : ${num } <br/>
</c:forEach>
<br/>
<c:forEach items="${requestScope.list }" var="list">
	list : ${list } <br/>
</c:forEach>
<br/>
<c:forEach items="${requestScope.map }" var="map">
	키 : ${map.key } - 값 : ${map.value } <br/>
</c:forEach>
<hr/>

<c:forEach var="i" begin="1" end="20" step="2">
출력 : ${i} <br>
</c:forEach>

</body>
</html>