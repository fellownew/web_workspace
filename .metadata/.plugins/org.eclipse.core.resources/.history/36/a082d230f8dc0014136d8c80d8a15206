	<script type="text/javascript">
		//한줄 주석 동일함.
		/*블럭 주석*/
		//hello1() 함수 선언
		function hello1(){
			window.alert("경고창을 띄운다");
			alert("한국어:안녕\n영어:Hello\n일본어:こんにちは");//window 생략됨.
		}
		//값으로 설정한 함수
		var hello2 = function(){
			alert("hello2 !!!!!!!!!!!");
		}
		//매개변수 함수
		function hello3(name,age){//매개변수에는 var 안함.
			alert(name+"님 안녕하세요");
			alert("나이 : "+age)
		}
		
		function sum
	</script>


