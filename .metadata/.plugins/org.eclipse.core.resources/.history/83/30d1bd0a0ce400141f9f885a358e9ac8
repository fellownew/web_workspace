<script type="text/javascript">
function selectorText(){
	$("a[href]").css("cursor","pointer"); // href 속성을 가진 a요소
	$("a[href='HelloServlet']").css("text-decoration","none");
	$("a[href^='http']").css("background-color","gray");//http로 시작하는것
	$("a[href$='net']").css("font-size","30px");//net으로 끝나는것.
	$("a[href*='n']").css("font-weight","bolder");//n이 포함되기만 하는것.
	
	$("a[href^!='http']").
}



</script>


</head>
<body>
<button onclick="selectorText();">selectorTest</button><p/>
