<%@ page contentType="text/html;charset=UTF-8"%>
	<jsp:useBean id="member3" class="servlet.vo.Member" scope="request"/>
	<jsp:useBean id="member4" class="servlet.vo.Member" scope="request"/>
<!DOCTYPE html>
<html>
<head>
<meta charset=UTF-8>
<title>Insert title here</title>
</head>
<body>

	member setProperty 이후 getProperty 이용<br/>
	<jsp:setProperty property="id" name="member" value="id-00"/>
	<jsp:setProperty property="password" name="member"  value="1234"/>
	<jsp:setProperty property="name" name="member" value="김철수"/>
	<jsp:setProperty property="age" name="member" value="22"/>
	<jsp:setProperty property="marriage" name="member" value="false"/>
	
	ID : <jsp:getProperty property="id" name="member"/><br/>
	password : <jsp:getProperty property="password" name="member"/><br/>
	이름 :<jsp:getProperty property="name" name="member"/><br/>
	나이 : <jsp:getProperty property="age" name="member"/><br/>
	결혼여부 : <jsp:getProperty property="marriage" name="member"/><p/>
	
	member4에 parameter 바로 넣은 후 getProperty로 표현<br/>
	<jsp:setProperty property="*" name="member4"/>
	ID : <jsp:getProperty property="id" name="member4"/><br/>
	password : <jsp:getProperty property="password" name="member4"/><br/>
	이름 :<jsp:getProperty property="name" name="member4"/><br/>
	나이 : <jsp:getProperty property="age" name="member4"/><br/>
	결혼여부 : <jsp:getProperty property="marriage" name="member4"/><p/>
	
	
		<a href="/myjsp/actiontag/request.jsp">요청페이지로 가기</a>
</body>
</html>