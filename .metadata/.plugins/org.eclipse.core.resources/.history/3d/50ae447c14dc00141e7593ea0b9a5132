package account.vo;

public class Account {
	private String id,password;
	private int amount;
	private AccountType type;
	public Account(String id, String password, int amount, AccountType type) {
		super();
		this.id = id;
		this.password = password;
		this.amount = amount;
		this.type = type;
	}
	
	public Account(String id, String password, int amount) {
		super();
		this.id = id;
		this.password = password;
		this.amount = amount;
	}

	
}
